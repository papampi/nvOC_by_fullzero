  screen -dmSL miner $HCD -a lyra2v2 -o stratum+tcp://$ZPOOL_LYRA2V2_POOL:$ZPOOL_LYRA2V2_PORT -u $BTC_ADDRESS -p $MINER_PWD -i $LYRA2V2_INTENSITY $CCMINER_OPTS
fi

if [[ $COIN == ZPOOL_LBRY ]]
then
