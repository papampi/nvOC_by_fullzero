# 0miner for nvOC v0019-2.1 by papampi

# DEV Mod Log:
# v=0001 : papampi: Initial Release
#
# v=0002 : papampi:
#          fix lbc
# v=0003 : papampi:
#          More NiceHash, MINER_PWD failsafe,
#          COIN_EXTENSION_ARGUMENTS for ethminer and genoil
# v=0004 : damNmad:
#          New Coins LUX, SUMO, DSR
# v=0005 : papampi:
#          New Coin ELLA,  XVG, GBX, CRC
# v=0006 : papampi:
#          EQUIHASH_MINER, bminer, ETHASH_MINER
# v=0007 : papampi:
#          Nicehash bug fixes
# v=0008 : papampi:
#          Move all miners to /home/m1/miners/
# v=0009 : papampi
#          Pool Wallet Address Format
# v=0010 : papampi
#          New Claymore Dual Mining
# v=0011 : damNmad
#          Added BTCP
# v=0012 : Falcon
#          Added CCMINER_OPTS, added RavenCoin (RVN)
# v=0013 : papampi
#          Changed EWBF HCD
# v=0014 : papampi
#          Code cleanup, NEOSCRYPT_MINER, LYRA2V2_MINER, SKUNK_MINER, SKEIN_MINER
# v=0015 : sizzlephizzle
#	   Declaration of ALGO, Unification of EQUIHASH & ETHASH mining by ALGO.

nvOC_Ver="nvOC v0019-2.1 - Community Release"
nvOC_0miner_ver="v0019-2.0.0015"   # Do not edit this

source /home/m1/1bash

# Set MINER_PWD if unset
if [ -z ${MINER_PWD+x} ]
then
  MINER_PWD="x"
fi

## Unify COIN Start
WALLET_ADDRESS_FORMAT="."
UPOOL="_POOL"
UPORT="_PORT"
UEXT="_EXTENSION_ARGUMENTS"
UADDR="_ADDRESS"
UWORK="_WORKER"
LAUNCH="screen -dmSL miner"
xpool=$COIN$UPOOL
xport=$COIN$UPORT
xadd=$COIN$UADDR
xwrk=$COIN$UWORK
xext=$COIN$UEXT
CLN=:
## Unify COIN End

## NICEHASH START
if [ $COIN == "NICE_NEOSCRYPT" -o $COIN == "NICE_LYRA2REV2" -o $COIN == "NICE_X11GOST" -o $COIN == "NICE_SKUNKHASH" -o $COIN == "NICE_CRYPTONIGHT"  -o $COIN == "NICE_EQUIHASH" -o $COIN == "NICE_ETHASH" ]
  then
    NICE_ADDRESS=$BTC_ADDRESS
	NICE_NEOSCRYPT_ADDRESS=$BTC_ADDRESS
	NICE_LYRA2REV2_ADDRESS=$BTC_ADDRESS
	NICE_X11GOST_ADDRESS=$BTC_ADDRESS
	NICE_SKUNKHASH_ADDRESS=$BTC_ADDRESS
	NICE_CRYPTONIGHT_ADDRESS=$BTC_ADDRESS
	NICE_EQUIHASH_ADDRESS=$BTC_ADDRESS
	NICE_ETHASH_ADDRESS=$BTC_ADDRESS
fi
## NICEHASH END

## IDENTIFY ALGORITHM START
if [ $COIN == "FTC" -o $COIN == "ORB" -o $COIN == "PXC" -o $COIN == "VIVO" -o $COIN == "TZC"  -o $COIN == "NICE_NEOSCRYPT" -o $COIN == "DSR" -o $COIN == "CRC" -o $COIN == "GBX" ]
  then
    ALGO="NEOSCRYPT"
fi
if [ $COIN == "ETH" -o $COIN == "ETC" -o $COIN == "EXP" -o $COIN == "UBQ" -o $COIN == "MUSIC" -o $COIN == "SOIL" -o $COIN == "MPH_ETHASH" -o $COIN == "PIRL" -o $COIN == "NICE_ETHASH" ]
  then
    ALGO="ETHASH"
fi
if [ $COIN == "BTG" -o $COIN == "ZEC" -o $COIN == "ZEN" -o $COIN == "HUSH" -o $COIN == "ZCL" -o $COIN == "BTCP" -o $COIN == "KMD" -o $COIN == "ZPOOL_EQUIHASH" -o $COIN == "MPH_EQUIHASH" -o $COIN == "NICE_EQUIHASH" -o $COIN == "VOTE"  ]
  then
    ALGO="EQUIHASH"
fi
 if [ $COIN == "SIGT" -o $COIN == "ZPOOL_SKUNK" -o $COIN == "ALTCOM" -o $COIN == "NICE_SKUNKHASH" ]
  then
    ALGO="SKUNK"
fi
if [ $COIN == "MONA" -o $COIN == "VTC" -o $COIN == "NICE_LYRA2REV2" -o $COIN == "XVG" ]
  then
    ALGO="LYRA2REV2"
fi
if [ $COIN == "XMR" -o $COIN == "KRB" -o $COIN == "ETN" -o $COIN == "NICE_CRYPTONIGHT" ]
  then
    ALGO="CRYPTONIGHT"
fi
if [ $COIN == "ZCOIN" ]
  then
    ALGO="LYRA2V2"
fi
## IDENTIFY ALGORITHM END	

## ETHASH START
if [[ $ALGO == ETHASH ]];
        then
                ## Declare Miner Start
						if [[ $ETHASH_MINER == GENOIL ]];
                        then
                                HCD="$LAUNCH /home/m1/miners/Genoil-U/ethminer -S "${!xpool}"$CLN"${!xport}" -O "${!xadd}"$WALLET_ADDRESS_FORMAT"${!xwrk}":x -SP 1 -U "${!xext}""
                        fi

                        if [[ $ETHASH_MINER == ETHMINER ]];
                        then
                                HCD="$LAUNCH /home/m1/miners/ethminer/latest/ethminer -S "${!xpool}"$CLN"${!xport}" -O "${!xadd}"$WALLET_ADDRESS_FORMAT"${!xwrk}":x -SP 1 -U "${!xext}""
                        fi

                        if [[ $ETHASH_MINER == CLAYMORE ]];
                        then
                                WALLET_ADDRESS_FORMAT=" -eworker "
                                HCD="$LAUNCH /home/m1/miners/claymore/$CLAYMORE_VERSION/ethdcrminer64 -epool "${!xpool}"$CLN"${!xport}" -ewal "${!xadd}"$WALLET_ADDRESS_FORMAT"${!xwrk}" -epsw x  -mode 1 -allcoins 1 -allpools 1 -dbg -1 "${!xext}""
                        fi
                ## Declare Miner End
$HCD
fi
## ETHASH END

## EQUIHASH START
if [[ $ALGO == EQUIHASH ]];
        then
        if [[ $EQUIHASH_MINER == ZM ]]
                then
                        HCD="$LAUNCH /home/m1/miners/dstm/latest/zm_miner --server "${!xpool}" --user "${!xadd}"$WALLET_ADDRESS_FORMAT"${!xwrk}" --port "${!xport}" --pass $MINER_PWD --time $ZM_OPTS"
        fi
        if [[ $EQUIHASH_MINER == EWBF ]]
                then
                        HCD="$LAUNCH /home/m1/miners/ewbf/v$EWBF_VERSION/miner --eexit 3 --fee $EWBF_PERCENT --pec --server "${!xpool}" --user "${!xadd}"$WALLET_ADDRESS_FORMAT"${!xwrk}" --pass $MINER_PWD --port "${!xport}" $EWBF_OPTS"
        fi
        if [[ $EQUIHASH_MINER == BMINER ]]
                then
                        HCD="$LAUNCH /home/m1/miners/bminer/latest/bminer -uri stratum://"${!xadd}"$WALLET_ADDRESS_FORMAT"${!xwrk}":$MINER_PWD@"${!xpool}"$CLN"${!xport}" $BMINER_OPTS"
        fi
        if [[ $EQUIHASH_MINER == BMINER_SSL ]]
                then
                        HCD="$LAUNCH /home/m1/miners/bminer/latest/bminer -uri stratum+ssl://"${!xadd}"$WALLET_ADDRESS_FORMAT"${!xwrk}":$MINER_PWD@"${!xpool}"$CLN"${!xport}" $BMINER_OPTS"
        fi
$HCD
fi
## EQUIHASH END


if [[ $COIN == CLAYMORE_DUAL ]]
then
  HCD=/home/m1/miners/claymore/$CLAYMORE_VERSION/ethdcrminer64
  E_ADDR="$DUAL_EWAL$WALLET_ADDRESS_FORMAT$DUAL_EWORKER"
  D_ADDR="$DUAL_DWAL$WALLET_ADDRESS_FORMAT$DUAL_DWORKER"
  screen -dmSL miner $HCD -epool $DUAL_EPOOL:$DUAL_EPORT -ewal $E_ADDR -epsw  $DUAL_EPSW -dpool stratum+tcp://$DUAL_DPOOL:$DUAL_DPORT -dwal $D_ADDR -dpsw $DUAL_DPSW -dcoin $DUAL_DCOIN -allpools $ALL_POOLS -dbg -1 -mport $MPORT -mpsw $MPSW $DUAL_ADDITIONAL_ARGUMENTS
fi

if [[ $COIN == RVN ]]
then
  HCD=/home/m1/miners/MSFTccminer/ccminer
  ADDR="$RVN_ADDRESS"
  screen -dmSL miner $HCD -a x16r -i $RVN_INTENSITY -o stratum+tcp://$RVN_POOL:$RVN_PORT -u $ADDR -p $MINER_PWD $CCMINER_OPTS
fi

if [[ $COIN == ONION ]]
then
  HCD=/home/m1/miners/TPccminer/ccminer
  ADDR="$ONION_ADDRESS"
  screen -dmSL miner $HCD -a x13 -o stratum+tcp://$ONION_POOL:$ONION_PORT -u $ADDR -p c=ONION $CCMINER_OPTS
fi

