  screen -c ${NVOC}/screenrc-miner -dmSL miner $HCD -a skunk -o stratum+tcp://$NICE_SKUNKHASH_POOL:$NICE_SKUNKHASH_PORT -u $ADDR -p x -i $SKUNK_INTENSITY $CCMINER_OPTS
fi

if [[ $COIN == NICE-CRYPTONIGHT ]]
then
  HCD=${NVOC}/miners/KTccminer-cryptonight/ccminer
  ADDR="$NICE_ADDRESS$WALLET_ADDRESS_FORMAT$NICE_WORKER"
  screen -c ${NVOC}/screenrc-miner -dmSL miner $HCD -o stratum+tcp://$NICE_CRYPTONIGHT_POOL:$NICE_CRYPTONIGHT_PORT -u $ADDR -p x -i $NICE_CRYPTONIGHT_INTENSITY $CCMINER_OPTS
fi
