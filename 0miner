# 0miner for nvOC v0019-2.1 by papampi

# DEV Mod Log:
# v=0001 : papampi: Initial Release
#
# v=0002 : papampi:
#          fix lbc
#
# v=0003 : papampi:
#          More NiceHash, MINER_PWD failsafe,
#          COIN_EXTENSION_ARGUMENTS for ethminer and genoil
#
# v=0004 : damNmad:
#          New Coins LUX, SUMO, DSR
# v=0005 : papampi:
#          New Coin ELLA,  XVG, GBX, CRC
#
# v=0006 : papampi:
#          EQUIHASH_MINER, bminer, ETHASH_MINER
# v=0007 : papampi:
#          Nicehash bug fixes
# v=0008 : papampi:
#          Move all miners to ${NVOC}/miners/
# v=0009 : papampi
#          Pool Wallet Address Format
# v=0010 : papampi
#          New Claymore Dual Mining
# v=0011 : damNmad
#          Added BTCP
# v=0012 : Falcon
#          Added CCMINER_OPTS, added RavenCoin (RVN)
# v=0013 : papampi
#          Changed EWBF HCD
# v=0014 : papampi
#          Code cleanup, NEOSCRYPT_MINER, LYRA2V2_MINER, SKUNK_MINER, SKEIN_MINER

nvOC_Ver="nvOC v0019-2.1 - Community Release"
nvOC_0miner_ver="v0019-2.0.0014"   # Do not edit this

source "${NVOC}/1bash"

# Set MINER_PWD if unset
if [ -z ${MINER_PWD+x} ]
then
  MINER_PWD="x"
fi

if [[ $COIN == CLAYMORE_DUAL ]]
then
  HCD=${NVOC}/miners/claymore/$CLAYMORE_VERSION/ethdcrminer64
  E_ADDR="$DUAL_EWAL$WALLET_ADDRESS_FORMAT$DUAL_EWORKER"
  D_ADDR="$DUAL_DWAL$WALLET_ADDRESS_FORMAT$DUAL_DWORKER"
  screen -c ${NVOC}/screenrc-miner -dmSL miner $HCD -epool $DUAL_EPOOL:$DUAL_EPORT -ewal $E_ADDR -epsw  $DUAL_EPSW -dpool stratum+tcp://$DUAL_DPOOL:$DUAL_DPORT -dwal $D_ADDR -dpsw $DUAL_DPSW -dcoin $DUAL_DCOIN -allpools $ALL_POOLS -dbg -1 -mport $MPORT -mpsw $MPSW $DUAL_ADDITIONAL_ARGUMENTS
fi

if [[ $COIN == RVN ]]
then
  HCD=${NVOC}/miners/MSFTccminer/ccminer
  ADDR="$RVN_ADDRESS"
  screen -c ${NVOC}/screenrc-miner -dmSL miner $HCD -a x16r -i $RVN_INTENSITY -o stratum+tcp://$RVN_POOL:$RVN_PORT -u $ADDR -p $MINER_PWD $CCMINER_OPTS
fi


if [[ $COIN == ONION ]]
then
  HCD=${NVOC}/miners/TPccminer/ccminer
  ADDR="$ONION_ADDRESS"
  screen -c ${NVOC}/screenrc-miner -dmSL miner $HCD -a x13 -o stratum+tcp://$ONION_POOL:$ONION_PORT -u $ADDR -p c=ONION $CCMINER_OPTS
fi

if [[ $COIN == SMART ]]
then
  HCD=${NVOC}/miners/ASccminer/ccminer
  ADDR="$SMART_ADDRESS$WALLET_ADDRESS_FORMAT$SMART_WORKER"
  screen -c ${NVOC}/screenrc-miner -dmSL miner $HCD -a keccak -o stratum+tcp://$SMART_POOL:$SMART_PORT -u $ADDR -p $MINER_PWD -i $SMART_INTENSITY $CCMINER_OPTS
fi

if [[ $COIN == BTG ]]
then
  BTGADDR="$BTG_ADDRESS$WALLET_ADDRESS_FORMAT$BTG_WORKER"

  if [[ $EQUIHASH_MINER == ZM ]]
  then
    HCD=${NVOC}/miners/dstm/latest/zm_miner
    screen -c ${NVOC}/screenrc-miner -dmSL miner $HCD --server $BTG_POOL --user $BTGADDR --pass $MINER_PWD --port $BTG_PORT  --time $ZM_OPTS
  fi
  if [[ $EQUIHASH_MINER == EWBF ]]
  then
    HCD=${NVOC}/miners/ewbf/v$EWBF_VERSION/miner
    screen -c ${NVOC}/screenrc-miner -dmSL miner $HCD --eexit 3 --fee $EWBF_PERCENT --pec --server $BTG_POOL --user $BTGADDR --pass $MINER_PWD --port $BTG_PORT  $EWBF_OPTS
  fi
  if [[ $EQUIHASH_MINER == BMINER ]]
  then
    HCD=${NVOC}/miners/bminer/latest/bminer
    screen -c ${NVOC}/screenrc-miner -dmSL miner $HCD -uri stratum://$BTGADDR:$MINER_PWD@$BTG_POOL:$BTG_PORT $BMINER_OPTS
  fi
  if [[ $EQUIHASH_MINER == BMINER_SSL ]]
  then
    HCD=${NVOC}/miners/bminer/latest/bminer
    screen -c ${NVOC}/screenrc-miner -dmSL miner $HCD -uri stratum+ssl://$BTGADDR:$MINER_PWD@$BTG_POOL:$BTG_PORT $BMINER_OPTS
  fi
fi

