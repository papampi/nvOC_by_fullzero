fi

if [[ $COIN == ZPOOL_BLAKE2S ]]
then
  HCD=/home/m1/miners/SPccminer/ccminer
  screen -dmSL miner $HCD -a blake2s -o stratum+tcp://$ZPOOL_BLAKE2S_POOL:$ZPOOL_BLAKE2S_PORT -u $BTC_ADDRESS -p $MINER_PWD -i $ZPOOL_BLAKE2S_INTENSITY $CCMINER_OPTS
