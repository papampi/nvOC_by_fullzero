  screen -dmSL miner $HCD -a lbry -o stratum+tcp://$ZPOOL_LBRY_POOL:$ZPOOL_LBRY_PORT -u $BTC_ADDRESS -p $MINER_PWD -i $ZPOOL_LBRY_INTENSITY $CCMINER_OPTS
fi

if [[ $COIN == ZPOOL_NEOSCRYPT ]]
then
