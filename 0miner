  ADDR="$FTC_ADDRESS$WALLET_ADDRESS_FORMAT$FTC_WORKER"
  screen -dmSL miner $HCD -a neoscrypt -o stratum+tcp://$FTC_POOL:$FTC_PORT -u $ADDR -p $MINER_PWD -i $NEOSCRYPT_INTENSITY $CCMINER_OPTS
